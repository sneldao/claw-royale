# CLAW ROYALE ENVIRONMENT TEMPLATE
# Copy this to .env and fill in your values
# DO NOT commit .env to git!

# ============================================================================
# AGENT WALLETS
# ============================================================================

# VOISSS Agent Wallet (Base mainnet)
# Generate with: node gen-new-wallet.cjs
# Store in: ~/.config/agent-wallets/voisss-agent.json
VOISS_AGENT_ADDRESS=
VOISS_AGENT_PRIVATE_KEY=

# Solana Devnet Wallet (for Colosseum hackathon)
# Generated with: solana-keygen grind --starts-with c:1
SOLANA_PRIVATE_KEY=

# ============================================================================
# API KEYS
# ============================================================================

# Bankr - AI-powered crypto trading
# Get key from: https://bankr.bot
BANKR_API_KEY=

# ElevenLabs - Voice generation
# Get key from: https://elevenlabs.io
ELEVENLABS_API_KEY=

# Neynar - Farcaster API
# Get key from: https:// Neynar
NEYNAR_API_KEY=

# ============================================================================
# RPC ENDPOINTS
# ============================================================================

# Solana
SOLANA_URL=https://api.devnet.solana.com

# Base
BASE_RPC_URL=https://mainnet.base.org
BASE_RPC_URL_DEVNET=https://sepolia.base.org

# ============================================================================
# APPLICATION CONFIG
# ============================================================================

# Fishnet Auth (Base/agent-apps)
FISHNET_AUTH_SECRET=

# Vercel
VERCEL_TOKEN=

# ============================================================================
# OPTIONAL / DEV
# ============================================================================

# Debug mode
DEBUG=false
LOG_LEVEL=info
